{
    "version": 2,
    "builds": [
        {
            "src": "api/webhook.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/chart.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/balance-chart.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/webhook",
            "dest": "/api/webhook.js"
        },
        {
            "src": "/api/chart",
            "dest": "/api/chart.js"
        },
        {
            "src": "/api/balance-chart",
            "dest": "/api/balance-chart.js"
        }
    ]
}